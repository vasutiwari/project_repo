<%=stylesheet_link_tag "ampnew"%>
<div id="sbox-window" style="" class="">
  <div class="sbox-bg-wrap"><div class="sbox-bg sbox-bg-n"></div><div class="sbox-bg sbox-bg-ne"></div>
    <div class="sbox-bg sbox-bg-e"></div><div class="sbox-bg sbox-bg-se"></div><div class="sbox-bg sbox-bg-s"></div>
    <div class="sbox-bg sbox-bg-sw"></div><div class="sbox-bg sbox-bg-w"></div><div class="sbox-bg sbox-bg-nw"></div>
  </div>
	<%= form_tag({:action => "new_accounting_system_type"}, :target => "upload_frame", :id => "upload_form1")  do %>
  <div id="sbox-content" style="visibility: visible; opacity: 1;color:black;width:550px;" class="sbox-content-iframe">
		<div class="dialog_light_center">
				<div class="dialog_second_center">
					<div class="uploadwrapper">
						<div class="uploadheaderow bottompadding">
							<div class="uploadheadercoll"><!-- <img border="0" width="16" height="16" src="/images/datasetup_icon15.png"/> --> Create New Accounting System Type</div>
						</div>
						<div class="uploadrow" style="width:480px;">
							<div class="uploadlabelrow" style="width: 477px;">
								<div class="newfolder_fieldlabel" style="margin-top:3px;">Accounting System Name:</div>
								<div class="newfolder_fieldinput" style="width:226px;">
									<input type="text" class="newfolder_fieldinput" name="acc_sys_name" id="folder_name"/>
								</div>
							</div>
						</div>
						<span id="success_error_msg" style="font-size:12px;padding-left:105px;"></span>
						<div class="uploadbutwrapper">
							<div style="margin-left: 92px;" class="greenbutton">
								<a href="javascript:;" onclick="if(blank_folder_name('folder_name')) { jQuery('#upload_form1').submit(); }" id="fol_button">Create</a><span>&nbsp;</span>
							</div>
							<div class="graybutton"><a href="javascript:;" onclick="Control.Modal.close();">Cancel</a><span>&nbsp;</span></div><br/></br/><br/>
						</div>
					</div>
			</div>
		</div>
  </div>
	<% end %>
	<iframe id="upload_frame" name="upload_frame" style="display: none"></iframe>
	<a id="sbox-btn-close" onclick='Control.Modal.close();return false;' style="cursor: pointer;"></a>
</div>