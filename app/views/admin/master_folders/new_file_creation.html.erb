<div id="sbox-window" style="" class=""><%# @nav_path = (@nav == 'edit') ? 'esp_' : '' %>
  <div class="sbox-bg-wrap"><div class="sbox-bg sbox-bg-n"></div><div class="sbox-bg sbox-bg-ne"></div>
    <div class="sbox-bg sbox-bg-e"></div><div class="sbox-bg sbox-bg-se"></div><div class="sbox-bg sbox-bg-s"></div>
    <div class="sbox-bg sbox-bg-sw"></div><div class="sbox-bg sbox-bg-w"></div><div class="sbox-bg sbox-bg-nw"></div>
  </div>
  <%= form_tag({:action => "new_file_upload",:portfolio_type_id=>params[:id],:parent_id => params[:folder_id]}, :multipart => true, :target => "upload_frame", :id => "upload_form3")  do %>
    <div id="sbox-content" style="visibility: visible; opacity: 1;color:black" class="sbox-content-iframe">
      <h3 class="light_head">Upload to '<%= @portfolio_folder.name.capitalize %>'</h3>
      <div class="dialog_light_center">
        <div class="dialog_second_center">
          <div class="status_light_error" id="error_display"></div>
          <div class="status_light_error" id="emp_file" style="display:none;">Please enter file name..</div>
          <div class="status_light_error" id="emp_due_date" style="display:none;"></div>
          <div class="status_light">
            <span class="simple_light_text">
              File Upload : &nbsp;  <%=file_field :master_excel_template,:uploaded_data,:size=>20%>
              <%=hidden_field_tag :master_folder_id, params[:folder_id] %>
            </span>
          </div>
          <div class="status">
          </div>
          <span style="text-align:right">
            <%= image_submit_tag("/images/step4btn_upload.jpg", :width=>"77", :height=>"33", :style=>"border:none;", :onClick=>"return blank_file_upload();") %>
          </span>
        </div>
      </div>
    </div>
  <% end %>
  <iframe id="upload_frame" name="upload_frame" style="display: none"></iframe>
  <a id="sbox-btn-close" onclick='Control.Modal.close();return false;' style="cursor: pointer;"></a></div>