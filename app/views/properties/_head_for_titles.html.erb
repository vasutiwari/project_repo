<%portfolio_id=portfolio_collection ? portfolio_collection.id : 0%>
<%note_id=note_collection ? note_collection.id : 0 %>
<% initial_tab_class = (params[:partial_disp].nil? || params[:partial_disp] != 'data_hub') %>
<% exec_tab = initial_tab_class ? 'executivesubtab' : 'deactivesubtab' %>
<% data_hub_tab = initial_tab_class ? 'deactivesubtab' : 'executivesubtab' %>
<div class="<%if is_leasing_agent%>deactivesubtab<%else%><%=exec_tab%><%end%>" id="p_review">
  <div class="left"></div>
  <div class="tabwrapper" style="width: 120px;">
    <%unless is_leasing_agent%>
      <a href="" onclick="sqftCalc=undefined;parSqftCalc=undefined;partial_page='portfolio_partial';active_title('performance_review');performanceReviewCalls('for_notes',{main_head_call:true});return false;">
        <div class="tabicon" style="text-align: center ! important;" id="p_review_img"><img src="/images/performanceicon1.png" width="30" height="25" border="0" /></div></a>
    <%else%>
      <div class="tabicon" style="text-align: center ! important;" id="p_review_img"><img src="/images/performanceicon1.png" width="30" height="25" border="0" /></div>
    <%end%>
    <div class="tablabel" style="padding:0 5px;">
      <%unless is_leasing_agent%>
        <a href="" onclick="sqftCalc=undefined;parSqftCalc=undefined;partial_page='portfolio_partial';active_title('performance_review');performanceReviewCalls('for_notes',{main_head_call:true});return false;">Reports &amp; Analysis</a>
      <%else%>
        Reports &amp; Analysis
      <%end%>
    </div>
  </div>
  <div class="right"></div>
</div>
<%if note_collection.leasing_type.eql?('Commercial')%>
  <div class="<%if !is_leasing_agent%>deactivesubtab<%else%><%=exec_tab%><%end%>" id="lease_view">
    <div class="left"></div>
    <div class="tabwrapper" style="padding:0 5px; width:86px" >
      <a href="" onclick="<%if is_leasing_agent%>active_title('property_pipeline');set_call('property_pipeline','<%=portfolio_collection.id%>',selected_item);<%else%>active_title('lease_management');set_call('lease_management','<%=portfolio_collection.id%>',selected_item);<%end%>return false;">
        <div class="tabicon" style="text-align: center ! important;"><img src="/images/lease_icon.png" width="30" height="25" border="0" />&nbsp;</div></a>
      <div class="tablabel" style="padding:0 5px;"><a onclick="<%if is_leasing_agent%>active_title('property_pipeline');set_call('property_pipeline','<%=portfolio_collection.id%>',selected_item);<%else%>active_title('lease_management');set_call('lease_management','<%=portfolio_collection.id%>',selected_item);<%end%>return false;">Lease Mgmt</a></div>
    </div>
    <div class="right"></div>
  </div>
<%end%>
