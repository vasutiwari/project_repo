<div class="<%= data_hub_tab %>" id="d_hub">
  <div class="left"></div>
  <div class="tabwrapper" style="width:120px;">
    <a href="" onclick="active_title('data_hub');show_data_hub_docs_real_estate(port_id,selected_item,'hide_del');return false;" class="bluecolor">
      <div class="tabicon"><img src="/images/executive_icon2.png" width="30" height="25" border="0" />&nbsp;</div></a>
    <div class="tablabel" style="padding:0 0px;"><a href="" onclick="active_title('data_hub');show_data_hub_docs_real_estate(port_id,selected_item,'hide_del');return false;" class="bluecolor">Files &amp; Collaboration</a></div>
  </div>
  <div class="right"></div>
</div>
<div class="deactivesubtab" id="p_view">
  <div class="left"></div>
  <div class="tabwrapper" style="width:100px;">
    <a href="" onclick="show_property_view_real_estate(<%=portfolio_id%>,selected_item);return false;">
      <div class="tabicon"><img src="/images/performanceicon3.png" width="30" height="25" border="0" />&nbsp;</div></a>
    <div class="tablabel" style="padding: 0pt 5px;"><a href="" onclick="show_property_view_real_estate(<%=portfolio_id%>,selected_item);return false;">Physical Details</a></div>

  </div>
  <div class="right"></div>
</div>
<div class="deactivesubtab" id="n_terms">
  <div class="left"></div>
  <div class="tabwrapper" style="width:100px;">
    <%unless is_leasing_agent%>
      <a href="" onclick="active_title('note_terms');set_call(active_call,'<%=portfolio_collection.id%>',selected_item);return false;">
        <div class="tabicon"><img src="/images/executive_icon4.png" width="30" height="25" border="0" />&nbsp;</div></a>
    <%else%>
      <div class="tabicon"><img src="/images/executive_icon4.png" width="30" height="25" border="0" />&nbsp;</div>
    <%end%>
    <div class="tablabel" style="padding: 0pt 5px;">
      <%unless is_leasing_agent%>
        <a href="" onclick="active_title('note_terms');set_call(active_call,'<%=portfolio_collection.id%>',selected_item);return false;">Debt Summary</a>
      <%else%>
                              Debt Summary
      <%end%>
    </div>
  </div>
  <div class="right"></div>
</div>
<div class="deactivesubtab" id="n_s_review">
  <div class="left"></div>
  <div class="tabwrapper" style="width:70px;">
    <%unless is_leasing_agent%>
      <a href="" onclick="active_title('property_settings');set_call('property_settings','<%=portfolio_collection.id%>',selected_item);return false;">
        <div class="tabicon"><img src="/images/performanceicon6.png" width="30" height="25" border="0" />&nbsp;</div></a>
    <%else%>
      <div class="tabicon"><img src="/images/performanceicon6.png" width="30" height="25" border="0" />&nbsp;</div>
    <%end%>
    <div class="tablabel" style="padding: 0pt 5px;">
      <%unless is_leasing_agent%>
        <a href="" onclick="active_title('property_settings');set_call('property_settings','<%=portfolio_collection.id%>',selected_item);return false;">Settings</a>
      <%else%> Settings <%end%>
